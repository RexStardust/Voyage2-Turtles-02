
{
  "name": "DevTab",
  "description": "New tab extension that provides tools for front-end developers",
  "manifest_version": 2,
  "version": "1.1.2",
  "icons": {
    "128": "assets/img/devtab5.png"
  },
  "homepage": "index.html",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "assets/img/devtab5.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["js/app.min.js"],
      "css": ["css/styles.css"],
      "run_at": "document_idle"
    }
  ]
}
